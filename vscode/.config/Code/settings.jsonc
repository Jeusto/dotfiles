{
  // ===========================================================================
  // Style
  // ===========================================================================
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "workbench.colorTheme": "One Sandbox Dark",
  "workbench.productIconTheme": "feather-vscode",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.theme": "specific",
  "window.title": "${rootName} ~ ${activeEditorShort}",
  "window.commandCenter": false,
  "window.menuBarVisibility": "toggle",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {},
  "editor.fontFamily": "'JetBrainsMono Nerd Font' , 'JetBrains Mono', Consolas, monospace",
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font' , 'JetBrains Mono', Consolas, monospace",

  // ===========================================================================
  // Editor
  // ===========================================================================

  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "editor.suggest.showStatusBar": true,
  "editor.autoClosingQuotes": "always",
  "editor.lineNumbers": "relative",
  "editor.guides.bracketPairs": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.parameterHints.enabled": false,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharacters": false,
  "editor.rulers": [80, 120],
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.cursorBlinking": "blink",
  "editor.stickyScroll.enabled": true,
  "editor.cursorSurroundingLines": 8,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
    // "source.organizeImports": "explicit"
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.formatOnSave": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": "off",
  "editor.occurrencesHighlightDelay": 20,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "editor.smoothScrolling": true,
  "editor.suggest.insertMode": "replace",
  "editor.wordWrap": "off",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.languageDetection": true,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "file:/**/*.csv": "jupyter-data-wrangler"
  },

  // ===========================================================================
  // Terminal
  // ===========================================================================
  "terminal.external.windowsExec": "C:\\Program Files\\Git\\git-bash.exe",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.persistentSessionScrollback": 250,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1",
    "CW_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.shellIntegration.decorationsEnabled": "overviewRuler",
  "terminal.integrated.scrollback": 2500,
  "terminal.integrated.commandsToSkipShell": [
    "-workbench.action.quickOpen",
    "workbench.action.terminal.kill"
  ],
  "terminal.integrated.env.linux": {},
  "terminal.integrated.env.windows": {},
  "terminal.integrated.stickyScroll.enabled": true,
  "terminal.integrated.suggest.enabled": true,

  // ===========================================================================
  // Quality of life improvements
  // ===========================================================================
  "window.openFilesInNewWindow": "default",
  "window.newWindowDimensions": "inherit",
  "explorer.confirmDragAndDrop": false,
  "problems.showCurrentInStatus": false,
  "security.workspace.trust.banner": "never",
  "search.searchEditor.doubleClickBehaviour": "selectWord",
  "security.workspace.trust.untrustedFiles": "open",
  "debug.onTaskErrors": "debugAnyway",
  "debug.toolBarLocation": "docked",
  "window.autoDetectColorScheme": true,
  "workbench.startupEditor": "none",
  "explorer.confirmDelete": false,
  "window.confirmSaveUntitledWorkspace": false,
  "workbench.experimental.enableNewProfilesUI": true,
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.tree.enableStickyScroll": true,
  "workbench.tree.renderIndentGuides": "always",
  "workspaceInStatusBar.clickCommand": "workbench.action.openRecent",
  "workspaceInStatusBar.priority": 0,

  // ===========================================================================
  // Git
  // ===========================================================================
  "git.autofetch": "all",
  "git.blame.editorDecoration.enabled": false,
  "git.blame.statusBarItem.enabled": true,
  "git.confirmEmptyCommits": true,
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "always",
  "scm.defaultViewMode": "tree",
  "scm.experimental.showSyncView": true,
  "scm.showHistoryGraph": false,

  // ===========================================================================
  // Languages
  // ===========================================================================

  // Go
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    }
  },
  "go.inlayHints.assignVariableTypes": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.constantValues": true,
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.rangeVariableTypes": true,
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "ui.semanticTokens": true
  },
  "go.formatTool": "goimports",

  // Javascript/Typescript
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.associations": {
    "*.ndjson": "jsonl",
    "*.dbclient-js": "javascript",
    "*.pl": "prolog",
    "*.jsx": "javascriptreact",
    "*.rmd": "markdown"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "javascript.preferences.renameMatchingJsxTags": true,
  "javascript.referencesCodeLens.enabled": false,
  "javascript.referencesCodeLens.showOnAllFunctions": false,
  "typescript.preferences.renameMatchingJsxTags": true,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.referencesCodeLens.showOnAllFunctions": false,

  // Other languages ===========================================================
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.format.enable": true,
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "[tex]": {
    "editor.wordWrap": "on"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[c]": {
    "editor.formatOnSave": true
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },

  // ===========================================================================
  // Copilot
  // ===========================================================================
  "github.copilot.nextEditSuggestions.enabled": true,
  "chat.agent.enabled": true,
  "chat.detectParticipant.enabled": true,
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "chat.experimental.quickQuestion.enable": true,
  "chat.mcp.discovery.enabled": true,
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "github.copilot.chat.inlineChatCompletionTrigger.enabled": true,
  "github.copilot.chat.inlineChatHint.enabled": true,
  "github.copilot.chat.search.semanticTextResults": true,
  "github.copilot.chat.welcomeMessage": "never",
  "github.copilot.editor.enableAutoCompletions": true,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true
  },

  // ===========================================================================
  // Vim
  // ===========================================================================
  "vim.showMarksInGutter": true,
  "vim.hlsearch": true,
  "vim.handleKeys": {
    "<C-p>": false,
    "<C-i>": false
  },
  "vim.visualstar": true,
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.enableNeovim": false,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "#61afef33",
  "vim.surround": true,
  "vim.neovimConfigPath": "~/.vscode-neovim.vim",
  "vim.neovimUseConfigFile": true,
  "vim.statusBarColors.normal": "#61afef",
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "~/.vimrc",
  "vim.showmodename": false,
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindingsNonRecursive": [],
  "vscode-neovim.mouseSelectionStartVisualMode": true,
  "vscode-neovim.neovimExecutablePaths.win32": "C:\\Users\\ARHUN.SADAY\\scoop\\shims\\nvim",
  "vscode-neovim.neovimWidth": 120,
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape"
    }
  },

  // ===========================================================================
  // Extensions
  // ===========================================================================

  // Code runner
  "code-runner.runInTerminal": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.executorMap": {
    "python": "python3 $dir$fileName",
    "c": "cd $dir && gcc -pthread $fileName -o $fileNameWithoutExt.out -Wall -Wextra -O3 -march=native && ./$fileNameWithoutExt.out",
    "cmat": "cd $workspaceRoot && make && LOG_LEVEL=TRACE ./bin/cmat $fullFileName --tos --ql -o ./bin/output.asm && ./mars.jar ./bin/output.asm",
    "cpp": "cd $dir && clang++ -std=c++17 -stdlib=libc++ $fileName -o $fileNameWithoutExt.out -Wall -Wextra -O3 -march=native && ./$fileNameWithoutExt.out"
  },

  // Errorlens
  "errorLens.enabled": true,
  "errorLens.messageEnabled": true,
  "errorLens.excludeBySource": ["ts(2307)"],
  "errorLens.messageBackgroundMode": "message",
  "errorLens.onSave": true,
  "errorLens.statusBarMessageTemplate": "✱ $message $source $code",
  "errorLens.messageTemplate": "$message $source $code",
  "errorLens.padding": "0rem 0.25rem",
  "errorLens.severityText": ["Warning", "Info", "Hint", "Error"],
  "errorLens.statusBarMessageEnabled": false,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.statusBarMessageType": "closestProblem",
  "errorLens.statusBarCommand": "copyMessage",
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.statusBarIconsUseBackground": false,
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info", "hint"],
  "errorLens.gutterIconSet": "squareRounded",
  "errorLens.gutterIconSize": "120%",

  // Gitlens
  "gitlens.views.stashes.files.layout": "tree",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "alternate",
  "gitlens.defaultDateFormat": "D MMM YY -- dddd",
  "gitlens.defaultTimeFormat": "hh:mm",
  "gitlens.defaultDateShortFormat": "D/MM/YY",
  "gitlens.defaultDateLocale": null,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": true,
    "suppressLineUncommittedWarning": true
  },
  "gitlens.graph.dimMergeCommits": true,
  "gitlens.graph.layout": "editor",
  "gitlens.telemetry.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.autolinks.enabled": false,
  "gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
  "gitlens.ai.model": "gitkraken",
  "gitlens.currentLine.enabled": false,
  "gitlens.graph.minimap.enabled": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.hovers.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.views.commitDetails.autolinks.enabled": false,
  "gitlens.views.commitDetails.autolinks.enhanced": false,
  "gitlens.views.scm.grouped.views": {
    "branches": true,
    "commits": true,
    "contributors": true,
    "launchpad": true,
    "remotes": true,
    "repositories": true,
    "searchAndCompare": true,
    "stashes": false,
    "tags": true,
    "worktrees": true
  },

  // Custom UI
  "custom-ui-style.stylesheet": {
    ".letterpress": "background-image: url(data:image/svg+xml;base64,) !important; opacity: 0.4;",
    ".monaco-workbench .part>.title": "padding-left: 2px !important; padding-right: 2px !important;",
    ".sidebar.right .composite-bar-container": "flex-direction: row-reverse !important;",
    ".title-label": "display: none !important",
    ".titlebar-left": "visibility: hidden !important"
  },
  "custom-ui-style.font.sansSerif": "JetBrainsMono Nerd Font, Jetbrains Mono, -apple-system",

  // Settings cycle
  "settings.cycle": [
    {
      "id": "copilot",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "github.copilot.enable": {
            "*": false
          },
          "workbench.colorCustomizations": {
            "editorLineNumber.activeForeground": "#61afef"
          }
        },
        {
          "github.copilot.enable": {
            "*": true
          },
          "workbench.colorCustomizations": {}
        }
      ]
    },
    {
      "id": "formatOnSave",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.formatOnSave": true
        },
        {
          "editor.formatOnSave": false
        }
      ]
    },
    {
      "id": "errorLens",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "errorLens.messageEnabled": true
          // "errorLens.enabled": true
        },
        {
          "errorLens.messageEnabled": false
          // "errorLens.enabled": false
        }
      ]
    },
    {
      "id": "inlayHints",
      "values": [
        {
          "editor.inlayHints.enabled": "on"
        },
        {
          "editor.inlayHints.enabled": "offUnlessPressed"
        }
      ]
    },
    {
      "id": "occurrencesHighlight",
      "values": [
        {
          "editor.occurrencesHighlight": "off"
        },
        {
          "editor.occurrencesHighlight": "multiFile"
        }
      ]
    }
  ],

  // Commands
  "commands.commands": {
    "Toggle ErrorLens": {
      "command": "settings.cycle.errorlensToggle",
      "icon": "error"
    },
    "Git Graph: View Git Graph": {
      "command": "git-graph.view",
      "icon": "git-compare",
      "statusBar": {
        "alignment": "left",
        "priority": -9
      }
    },
    "Toggle Copilot": {
      "command": "settings.cycle.copilot",
      "icon": "sparkle",
      "statusBar": {
        "alignment": "left",
        "priority": -8
      }
    },
    "Toggle Format on Save": {
      "command": "settings.cycle.formatOnSave",
      "icon": "check-all",
      "statusBar": {
        "alignment": "left",
        "priority": -10
      }
    },
    "Toggle multiple settings": {
      "sequence": [
        "editor.action.toggleMinimap",
        "workbench.action.toggleStatusbarVisibility"
      ]
    },
    "Toggle zen mode": "workbench.action.toggleZenMode",
    "Typing": {
      "args": {
        "snippet": "Typing... "
      },
      "command": "editor.action.insertSnippet"
    },
    "View: Reopen closed editor": "workbench.action.reopenClosedEditor"
  },

  // Other extensions ==========================================================
  "tailwind-fold.autoFold": true,
  "python.formatting.provider": "black",
  "python.linting.enabled": false,
  "python.linting.pylintEnabled": true,
  "react-native-tools.showUserTips": false,
  "hidefiles.version": "2.0.0",
  "jump.display.backgroundColor": "#282c34",
  "jump.display.color": "#e5c07b",
  "jump.primaryCharset": "asdfghjklercvbn",
  "labeledBookmarks.defaultShape": "heart",
  "leetcode.defaultLanguage": "typescript",
  "ltex.language": "fr",
  "markdown.extension.toc.levels": "2..4",
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.files.associations": {
    "*.cmat": "objective-c",
    "*.exp": "log"
  },
  "material-icon-theme.files.color": "#90a4ae",
  "material-icon-theme.hidesExplorerArrows": false,
  "material-icon-theme.opacity": 0.9,
  "material-icon-theme.saturation": 0.9,
  "notebook.cellToolbarVisibility": "hover",
  "notebook.lineNumbers": "on",
  "fileNestingUpdater.promptOnAutoUpdate": false,
  "angular.forceStrictTemplates": true,
  "betterGitLineBlame.showInlineAnnotations": false,
  "betterGitLineBlame.showStatusBarItem": false,
  "bookmarks.gutterIconBorderColor": "",
  "bookmarks.keepBookmarksOnLineDelete": true,
  "bookmarks.label.suggestion": "useWhenSelected",
  "database-client.autoSync": true,
  "docker.extension.dockerEngineAvailabilityPrompt": false,
  "docker.extension.enableComposeLanguageServer": false,
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "emulator.emulatorPath": "~/Android/Sdk/emulator",
  "fileNestingUpdater.autoUpdateInterval": 600,
  "githubPullRequests.pullBranch": "never",
  "todo-tree.general.showIconsInsteadOfTagsInStatusBar": true,
  "todo-tree.general.statusBar": "total",
  "todo-tree.highlights.enabled": true,
  "totalTypeScript.hiddenTips": [
    "basic-types",
    "ts-object-type",
    "type-alias-declaration",
    "function-return-type",
    "union-type",
    "typing-function-parameters",
    "interface-declaration",
    "array-type",
    "literal-type",
    "passing-generics-to-types",
    "pick-utility-type",
    "optional-object-property",
    "as-assertion",
    "any-type",
    "variable-type-annotation",
    "null-keyword",
    "record-utility-type",
    "undefined-keyword"
  ],
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": false,
  "binocular.command.commands": [
    {
      "commandIdentifier": "Lazygit",
      "shellCommand": "lazygit"
    },
    {
      "commandIdentifier": "Projects",
      "shellCommand": "find ~/depots/ ~/repos ~/tests ~/ -mindepth 1 -maxdepth 1 -type d -not -path '*/.*' | fzf | xargs -I{} code -r {}"
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
