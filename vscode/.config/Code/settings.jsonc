  // ===========================================================================
  // Vim keybindings
  // ===========================================================================
  "vim.normalModeKeyBindings": [
    {
      "before": ["<esc>"],
      "commands": [
        ":nohlsearch",
        "closeMarkersNavigation",
        "editor.closeTestPeek"
      ],
      "silent": true
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["code-runner.run"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.quickOpen"],
      "silent": true
    },
    {
      "before": ["g", "a"],
      "commands": ["keyboard-quickfix.openQuickFix"],
      "silent": true
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["bookmarks.toggle"],
      "silent": true
    },
    // LSP
    {
      "before": ["<leader>", "l", "j"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "l", "k"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["<leader>", "l", "d"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "l", "f"],
      "commands": ["editor.action.formatDocument"],
      "silent": true
    },
    {
      "before": ["<leader>", "l,", "a"],
      "commands": ["editor.action.sourceAction", "source.addMissingImports"]
    },
    {
      "before": ["<leader>", "l", "o"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["<leader>", "l", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "l", "c"],
      "commands": ["errorLens.copyProblemMessage"]
    },

    {
      "before": ["<leader>", "l", "w"],
      "commands": [
        "editor.action.formatDocument",
        "workbench.action.files.save"
      ]
    },
    // Test runner
    {
      "before": ["<leader>", "t", "t"],
      "commands": ["testing.runAtCursor"]
    },
    // Git
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["git.openChange"]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": ["git.checkout"]
    },
    {
      "before": ["<leader>", "g", "l"],
      "commands": ["betterGitLineBlame.toggleInlineAnnotations"],
      "silent": true
    },
    {
      "before": ["<leader>", "g", "j"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["<leader>", "g", "k"],
      "commands": ["workbench.action.editor.previousChange"]
    },
    // {
    //   "before": ["<leader>", "g", "p"],
    //   "commands": ["editor.action.dirtydiff.next"]
    // },
    {
